<?xml version="1.0" encoding="UTF-8"?>
<!-- <!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd"> -->
<suite name="Suite">
    <listeners>
        <listener class-name="MiniProject.AutomationReport"></listener>
    </listeners>
    <test name="chrometest">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="MiniProject.Main"/>
        </classes>
    </test>
    <test name="edgetest">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="MiniProject.Main"/>
        </classes>
    </test>
</suite>